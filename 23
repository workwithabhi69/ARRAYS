#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    int maxProd = arr[0];
    int currMax = arr[0];
    int currMin = arr[0];

    for (int i = 1; i < n; i++) {
        if (arr[i] < 0)
            swap(currMax, currMin);

        currMax = max(arr[i], currMax * arr[i]);
        currMin = min(arr[i], currMin * arr[i]);

        maxProd = max(maxProd, currMax);
    }

    cout << maxProd;
    return 0;
}
