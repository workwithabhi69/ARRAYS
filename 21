#include <iostream>
#include <vector>
#include <unordered_set>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    unordered_set<int> prefixSums;
    int sum = 0;
    bool found = false;

    for (int i = 0; i < n; i++) {
        sum += arr[i];
        if (sum == 0 || prefixSums.find(sum) != prefixSums.end()) {
            found = true;
            break;
        }
        prefixSums.insert(sum);
    }

    if (found)
        cout << "Yes";
    else
        cout << "No";

    return 0;
}
