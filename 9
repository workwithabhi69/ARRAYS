#include <iostream>
#include <algorithm>
#include <climits>
using namespace std;

int main() {
    int n, k;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++)
        cin >> arr[i];
    cin >> k;

    sort(arr, arr + n);

    int ans = arr[n - 1] - arr[0]; 
    int smallest = arr[0] + k;
    int largest = arr[n - 1] - k;

    for (int i = 0; i < n - 1; i++) {
        int minVal = min(smallest, arr[i + 1] - k);
        int maxVal = max(largest, arr[i] + k);

        if (minVal < 0)
            continue; 

        ans = min(ans, maxVal - minVal);
    }

    cout << "Minimum possible difference: " << ans;

    return 0;
}
