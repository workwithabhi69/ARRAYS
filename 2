#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    int arr[n];
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    int minVal, maxVal;
    int i;

    // If even number of elements
    if (n % 2 == 0) {
        if (arr[0] > arr[1]) {
            maxVal = arr[0];
            minVal = arr[1];
        } else {
            maxVal = arr[1];
            minVal = arr[0];
        }
        i = 2;
    } 
    // If odd number of elements
    else {
        maxVal = minVal = arr[0];
        i = 1;
    }

    while (i < n - 1) {
        if (arr[i] > arr[i + 1]) {
            if (arr[i] > maxVal)
                maxVal = arr[i];
            if (arr[i + 1] < minVal)
                minVal = arr[i + 1];
        } else {
            if (arr[i + 1] > maxVal)
                maxVal = arr[i + 1];
            if (arr[i] < minVal)
                minVal = arr[i];
        }
        i += 2;
    }

    cout << "Minimum el

