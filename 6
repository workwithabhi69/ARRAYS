#include <iostream>
#include <set>
using namespace std;

int main() {
    int n, m;
    cin >> n;
    int a[n];
    for (int i = 0; i < n; i++)
        cin >> a[i];

    cin >> m;
    int b[m];
    for (int i = 0; i < m; i++)
        cin >> b[i];

    set<int> unionSet;

    for (int i = 0; i < n; i++)
        unionSet.insert(a[i]);

    for (int i = 0; i < m; i++)
        unionSet.insert(b[i]);

    cout << "Number of elements in union: " << unionSet.size();

    return 0;
}
