#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> arr(n);
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    vector<int> pos, neg;
    for (int x : arr) {
        if (x >= 0)
            pos.push_back(x);
        else
            neg.push_back(x);
    }

    int i = 0, j = 0, k = 0;
    bool turnPos = true;

    while (i < pos.size() && j < neg.size()) {
        if (turnPos)
            arr[k++] = pos[i++];
        else
            arr[k++] = neg[j++];
        turnPos = !turnPos;
    }

    while (i < pos.size())
        arr[k++] = pos[i++];
    while (j < neg.size())
        arr[k++] = neg[j++];

    for (int x : arr)
        cout << x << " ";

    return 0;
}
